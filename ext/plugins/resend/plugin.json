{
  "schema_version": "v1",
  "name": "Resend Email",
  "description": "Send transactional emails through the Resend API.",
  "plugin_key": "external_resend_email",
  "action_types": ["resend_email"],
  "runtime": {
    "command": "./run.sh",
    "args": [],
    "env": {
      "RESEND_API_KEY": "${AGENT_RUNTIME_RESEND_API_KEY}",
      "RESEND_FROM": "${AGENT_RUNTIME_RESEND_FROM}",
      "RESEND_API_BASE": "${AGENT_RUNTIME_RESEND_API_BASE}"
    },
    "timeout_seconds": 45,
    "isolation": {
      "mode": "uv",
      "project": ".",
      "warm_on_bootstrap": true,
      "locked": true
    }
  }
}
